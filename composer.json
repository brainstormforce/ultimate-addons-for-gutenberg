{
    "name": "brainstormforce/ultimate-addons-for-gutenberg",
    "description": "Power-up the Gutenberg editor with advanced and powerful blocks that help you build websites in minutes!",
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
        "wp-coding-standards/wpcs": "0.14.1",
        "phpcompatibility/phpcompatibility-wp": "*"
    },
    "scripts": {
        "format": "phpcbf --standard=phpcs.xml.dist --report-summary --report-source",
        "lint": "phpcs --standard=phpcs.xml.dist --report-summary --report-source"
    }
}
